        {
          "comment": "Create a font style.",
          "returns": "Font",
          "returns_comment": false,
          "parameters": [
            {
              "name": "size",
              "comment": "font size. Defaults is 12px.",
              "type": "number",
              "default": "12"
            },
            {
              "name": "face",
              "comment": "Optional font-family, use css-like string such as \"Helvetica\" or \"Helvetica, sans-serif\". Default is \"sans-serif\".",
              "type": "string",
              "default": "\"sans-serif\""
            },
            {
              "name": "weight",
              "comment": "Optional font weight such as \"bold\". Default is \"\" (none).",
              "type": "string",
              "default": "\"\""
            },
            {
              "name": "style",
              "comment": "Optional font style such as \"italic\". Default is \"\" (none).",
              "type": "string",
              "default": "\"\""
            },
            {
              "name": "lineHeight",
              "comment": "Optional line height. Default is 1.5.",
              "type": "number",
              "default": "1.5"
            }
          ],
          "tags": [
            {
              "tag": "example",
              "text": "`new Font(12, \"Frutiger, sans-serif\", \"bold\", \"underline\", 1.5)`\n"
